{{#if resume.languages.length}}
<section id="languages" class="item">
	<h2><i class="fa fa-language left"></i> LANGUAGES</h2>
	<div class="languages_container">
	{{#each resume.languages}}
	<div class="languages_item">
	<h3>{{language}}</h3>
	<h4>Fluency: {{fluency}}</h4>
	</div>
	{{/each}}
	</div>
</section>
{{/if}}