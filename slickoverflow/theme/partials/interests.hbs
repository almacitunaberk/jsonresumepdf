{{#if resume.interests.length}}
<section class="section">
	<header>
		<h2 class='section-title'>Interests</h2>
	</header>
	<section id="skills">
		{{#each resume.interests}}
		<div class="item">
			{{#if name}}
			<h3 class="name">
				{{name}}
			</h3>
			{{/if}}
			{{#if keywords.length}}
			<ul class="keywords">
				{{#each keywords}}
				<li>{{.}}</li>
				{{/each}}
			</ul>
			{{/if}}
		</div>
		{{/each}}
	</section>
</section>
{{/if}}
