{{#if resume.interests.length }}
<div id="skills" class="col-md-12">
	<hr class="col-md-9 col-md-offset-3" />
	<div class="container-fluid no-break">
		<div class="row">
			<div class="col-md-3">
				<h3>Interests</h3>
			</div>
			<div class="col-md-9 skills_container">
			{{#each resume.interests}}
				<div class="skills_item">
						<h4 class="name">
							{{name}}
						</h4>
				</div>
			{{/each}}
			</div>
		</div>
	</div>
</div>
{{/if}}